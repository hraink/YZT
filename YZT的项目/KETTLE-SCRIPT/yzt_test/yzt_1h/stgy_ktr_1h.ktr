    <sql>SELECT&#xd;&#xa;	&#x27;0100&#x27; &#x7c;&#x7c; PPI.PLATE_CODE AS PLATE_CODE,&#xd;&#xa;	SC.STOCK_CONC_REL SEC_RELA_LEVEL,&#xd;&#xa;	CASE SBI.SEC_MAR_PAR&#xd;&#xa;WHEN 1 THEN&#xd;&#xa;	&#x27;00&#x27;&#xd;&#xa;WHEN 2 THEN&#xd;&#xa;	&#x27;01&#x27;&#xd;&#xa;END &#x7c;&#x7c; LPAD &#x28;SBI.STK_CODE, 8, &#x27;0&#x27;&#x29; CONC_RELA_STK,&#xd;&#xa; ROW_NUMBER &#x28;&#x29; OVER &#x28;&#xd;&#xa;	PARTITION BY SC.CONC_UNI_CODE&#xd;&#xa;	ORDER BY&#xd;&#xa;		SC.STOCK_CONC_REL DESC,SBI.STK_CODE&#xd;&#xa;&#x29; RN&#xd;&#xa;FROM&#xd;&#xa;	UPAPP.COM_CONC_INFO CCI&#xd;&#xa;LEFT JOIN UPAPP.PUB_PLATE_INFO PPI ON PPI.RELA_UNI_CODE &#x3d; CCI.CONC_UNI_CODE&#xd;&#xa;LEFT JOIN UPAPP.STOCK_CONC SC ON CCI.CONC_UNI_CODE &#x3d; SC.CONC_UNI_CODE&#xd;&#xa;JOIN UPAPP.STK_BASIC_INFO SBI ON SBI.COM_UNI_CODE &#x3d; SC.COM_UNI_CODE&#xd;&#xa;AND SBI.LIST_DATE &#x3c;&#x3d; ADD_MONTHS &#x28;CURRENT_DATE ,- 12&#x29;&#xd;&#xa;WHERE&#xd;&#xa;	SC.ISVALID &#x3d; 1&#xd;&#xa;AND SBI.ISVALID &#x3d; 1&#xd;&#xa;AND CCI.ISVALID &#x3d; 1&#xd;&#xa;AND PPI.ISVALID &#x3d; 1&#xd;&#xa;AND PPI.IS_VALID_PAR &#x3d; 1&#xd;&#xa;AND PPI.PLATE_UNI_CODE LIKE &#x27;500416&#x25;&#x27;&#xd;&#xa;AND SBI.SEC_MAR_PAR IN &#x28;1, 2&#x29;&#xd;&#xa;AND SBI.STK_TYPE_PAR &#x3d; 1&#xd;&#xa;AND SC.STOCK_CONC_REL &#x3e;&#x3d; 0&#xd;&#xa;AND NVL &#x28;&#xd;&#xa;	CCI.VALID_DATE,&#xd;&#xa;	DATE &#x27;5000-01-01&#x27;&#xd;&#xa;&#x29; &#x3e; TRUNC &#x28;SYSDATE&#x29; </sql>
