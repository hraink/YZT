    <sql>SELECT CASE B.SEC_MAR_PAR&#xd;&#xa;         WHEN 1 THEN&#xd;&#xa;          &#x27;00&#x27;&#xd;&#xa;         WHEN 2 THEN&#xd;&#xa;          &#x27;01&#x27;&#xd;&#xa;       END &#x7c;&#x7c; LPAD&#x28;B.STK_CODE, 8, &#x27;0&#x27;&#x29; AS STK_CODE,&#xd;&#xa;       DECODE&#x28;A.STK_HOT_INDEX, NULL, 0, A.STK_HOT_INDEX&#x29; STK_HOT_INDEX,&#xd;&#xa;       DECODE&#x28;A.HOT_INDEX_CHG_RATIO, NULL, 0, A.HOT_INDEX_CHG_RATIO&#x29; HOT_INDEX_CHG_RATIO,&#xd;&#xa;       B.STK_SHORT_NAME,&#xd;&#xa;       A.COUNT_time COUNT_DATE &#xd;&#xa;  FROM UPAPP.STK_BASIC_INFO B&#xd;&#xa;  LEFT JOIN &#x28;SELECT STK_UNI_CODE,&#xd;&#xa;                    COUNT_time,&#xd;&#xa;                    STK_HOT_INDEX,&#xd;&#xa;                    HOT_INDEX_CHG_RATIO&#xd;&#xa;               FROM UPAPP.stk_hot_index_Last A&#xd;&#xa;              WHERE A.ISVALID &#x3d; 1&#xd;&#xa;              &#x29; A&#xd;&#xa;    ON B.STK_UNI_CODE &#x3d; A.STK_UNI_CODE&#xd;&#xa; WHERE B.ISVALID &#x3d; 1&#xd;&#xa;   AND B.STK_CODE IS NOT NULL&#xd;&#xa;   AND B.SEC_MAR_PAR IN &#x28;1, 2&#x29;&#xd;&#xa;   AND B.STK_TYPE_PAR &#x3d; 1&#xd;&#xa;   AND SUBSTR&#x28;STK_CODE, 0, 1&#x29; &#x3c;&#x3e; &#x27;A&#x27;AND B.LIST_DATE&#x3c;&#x3d;ADD_MONTHS&#x28;CURRENT_DATE,-12&#x29;&#xd;&#xa;</sql>
