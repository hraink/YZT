<% include ./common/header %>
<title><%=title%></title>
<link rel="stylesheet" href="/css/bootstrap.min.css" />
<script src="/js/jquery-1.11.3.min.js"></script>
<script>
    $(function(){
        $("#ifrm").css("height",window.screen.height-200);
        $(".nav").find("li").each(function(){
            $(this).click(function(){
                var mType = $(this).attr("id");
                $(".nav").find("li").removeClass("active");
                $(this).addClass("active");
                if(mType=='day'){
                    $("#ifrm").attr("src","/html/a.html");
                }else{
                    $("#ifrm").attr("src","/html/b.html");
                }
            });
        });

        $("#addStock").click(function(){
            window.location.href = '/dayStocks/toAdd';
        });
    });
</script>
</head>
<body style="height: 100%">
    <div id="a" style="width:20%;height: 100%;float:left;">
        <%include pageMenu.ejs%>
    </div>
    <input type="hidden" id="mType" value="day">
    <iframe id="ifrm" style="width:70%;height: 100%;float:left;border: 0px;" src="/html/a.html"/>



<% include ./common/footer %>